{
  "name": "AceNote",
  "version": "0.1.0",
  "author": {
    "name": "xinsi",
    "email": "aceysx@163.com"
  },
  "private": true,
  "main": "main.js",
  "dependencies": {
    "antd": "^3.26.4",
    "codemirror": "^5.50.0",
    "electron-is-dev": "^1.1.0",
    "hypermd": "^0.3.11",
    "katex": "^0.11.1",
    "less-loader": "^5.0.0",
    "marked": "^0.8.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.3.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "packager": "electron-packager .  AceNote --overwrite --platform=darwin --arch=x64  --prune=true --out=outputs",
    "dist": "electron-builder --mac --x64"
  },
  "build": {
    "appId": "com.acey.aceNote",
    "productName": "AceNote",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "target": [
        "dmg"
      ],
      "category": "public.app-category.utilities"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "extends": null
  },
  "homepage": ".",
  "devDependencies": {
    "babel-plugin-import": "^1.12.0",
    "customize-cra": "^0.2.13",
    "electron-builder": "^22.3.2",
    "electron-packager": "^14.2.0",
    "less": "^3.10.3",
    "react-app-rewire-less": "^2.1.3",
    "react-app-rewired": "^2.1.3",
    "electron": "^7.1.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
